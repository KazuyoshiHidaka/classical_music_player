<div class="container">
  <% @videos[:items].each_with_index do |item, i| %>
    <% id = item.dig(:id, :video_id) %>
    <div class="my-5 embed-responsive embed-responsive-16by9">
      <iframe class="embed-responsive-item" src="https://www.youtube.com/embed/<%= id %>" allowfullscreen></iframe>
    </div>
  <% end %>
</div>
