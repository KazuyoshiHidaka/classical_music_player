<% selected_song ||= nil %>
<% all_songs_list_parent_class ||= nil %>
<% prefix = parent_container_name %>
<ul class="nav nav-tabs" id="<%= prefix %>SongsListsTab" role="tablist">
  <li class="nav-item" role="presentation">
    <a class="nav-link active" id="<%= prefix %>AllSongsTab" data-toggle="tab" href="#<%= prefix %>AllSongsTabContent"
       role="tab" aria-controls="<%= prefix %>AllSongsTabContent" aria-selected="true">
      一覧
    </a>
  </li>
</ul>
<div class="tab-content" id="SongsListsTabContent">
  <div class="tab-pane fade show active" id="<%= prefix %>AllSongsTabContent" role="tabpanel"
      　aria-labelledby="<%= prefix %>AllSongsTab">
    <%= render 'share/songs_lists/all', selected_song: all_songs_list_selected_song, 
                                        parent_class: all_songs_list_parent_class %>
  </div>
</div>
